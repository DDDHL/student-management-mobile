{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"app-plus": {
					"titleView": false,
					"bounce": "none"
				}
			}
		},
		{
			"path": "pages/vaction/vaction",
			"style": {
				"navigationStyle": "custom",
				"app-plus": {
					"titleView": false,
					"bounce": "none"
				}
			}

		}, {
			"path": "pages/service/service",
			"style": {
				"navigationBarTitleText": "服务",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#97d9e1"
			}

		}, {
			"path": "pages/user/user",
			"style": {
				"backgroundColor": "#99d8e1",
				"navigationStyle": "custom",
				"app-plus": {
					"titleView": false,
					"bounce": "none"
				}
			}

		}, {
			"path": "pages/userInfo/userInfo",
			"style": {
				"navigationBarTitleText": "修改个人信息",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#97d9e1",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/wechatLogin/wechatLogin",
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/appLogin/appLogin",
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/vactionList/vactionList",
			"style": {
				"navigationBarTitleText": "休假单",
				"enablePullDownRefresh": true
				
			}
		}
	],
	"globalStyle": {
		"backgroundColor": "#fff",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#97d9e1"
	},
	"tabBar": {
		"backgroundColor": "#fff",
		"borderStyle": "black",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/logo/index.png",
			"selectedIconPath": "static/logo/index_sc.png",
			"text": "主页"
		}, {
			"pagePath": "pages/vaction/vaction",
			"iconPath": "static/logo/vaction.png",
			"selectedIconPath": "static/logo/vaction_sc.png",
			"text": "休假"
		}, {
			"pagePath": "pages/service/service",
			"iconPath": "static/logo/tools.png",
			"selectedIconPath": "static/logo/tools_sc.png",
			"text": "服务"
		}, {
			"pagePath": "pages/user/user",
			"iconPath": "static/logo/user.png",
			"selectedIconPath": "static/logo/user_sc.png",
			"text": "我的"
		}]
	},
	"condition": {
		"current": 0,
		"list": [{
				"name": "user",
				"path": "pages/user/user"
			},
			{
				"name": "vaction",
				"path": "pages/vaction/vaction"
			},
			{
				"name": "login",
				"path": "pages/appLogin/appLogin"
			}

		]
	}
}
